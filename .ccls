%g++
%cpp -std=gnu++17

-pthread
-std=c++17

-I./src
-I./third_party/rapidjson/include
-I./third_party/khala/include
-I./third_party/uriparser/include
-I./third_party/spdlog/include

-Wall
-Wextra
-Wtrigraphs
-Wuninitialized
-Wwrite-strings
-Wpointer-arith
-Wredundant-decls
-Wno-unused-parameter
-Wmissing-include-dirs
-Wno-missing-field-initializers
-Wvla
-Wnon-virtual-dtor
-Woverloaded-virtual
-Wno-invalid-offsetof
-Werror=non-virtual-dtor
-Werror
-Wformat=2
-Wstrict-aliasing=3

